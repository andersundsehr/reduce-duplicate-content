{
  "name": "andersundsehr/reduce-duplicate-content",
  "description": "redirect if page has a / at the end (or not). (reduce Duplicate Content)",
  "type": "typo3-cms-extension",
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-core": "^11.5.11 || ^12.0",
    "php": "~8.1 || ~8.2"
  },
  "autoload": {
    "psr-4": {
      "AUS\\ReduceDuplicateContent\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "reduce_duplicate_content"
    }
  },
  "require-dev": {
    "pluswerk/grumphp-config": "^6.4",
    "saschaegerer/phpstan-typo3": "^1.8.2",
    "ssch/typo3-rector": "^1.1.3"
  },
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true,
      "phpro/grumphp": true,
      "phpstan/extension-installer": true,
      "pluswerk/grumphp-config": true
    }
  }
}
